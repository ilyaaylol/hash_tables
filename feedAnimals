def feedAnimals(animals, food):
    if len(animals) == 0 or len(food) == 0:
        return False
    animals.sort()
    food.sort()
    count = 0
    for f in food:
        if f >= animals[count]:
            count += 1
        if count == len(animals):
            break
    return count


print(feedAnimals([2, 10, 5], [2]))
